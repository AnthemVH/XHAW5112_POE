{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
      "jsx": "react-native",
      "moduleResolution": "node",
      "target": "es6",
      "lib": ["es6", "dom"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "resolveJsonModule": true
    },
    "include": [
      "src/**/*",
      "node_modules/react-native-web"
    ]
  }
  

